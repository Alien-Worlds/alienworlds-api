{
  "name": "alienworlds-api",
  "version": "1.0.0",
  "description": "API for AlienWorlds data",
  "main": "index.js",
  "scripts": {
    "test:unit": "jest --config=jest.config.unit.js",
    "test:api": "jest --config=jest.config.api.js",
    "build": "node ./scripts/create-env.js && yarn clean && tsc && yarn abis",
    "build-tests:api": "node ./scripts/create-env.js && yarn clean && tsc",
    "clean": "rm -rf ./dist",
    "abis": "mkdir -p ./dist/abis && node dist/fetch_abis.js ./dist/abis",
    "mongo-indexes": "node dist/mongo_setup.js",
    "api": "node dist/index.js",
    "blockrange": "node dist/blockrange.js",
    "filler": "node dist/filler.js",
    "processor": "node dist/processor.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@eosdacio/eosio-statereceiver": "^1.1.0",
    "@eosrio/node-abieos": "^2.1.0-beta.0",
    "@types/node": "^14.14.22",
    "amqplib": "^0.6.0",
    "atomicassets": "^1.2.2",
    "axon": "^2.0.3",
    "commander": "^7.0.0",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "eosjs": "^21.0.3",
    "ethers": "^5.1.0",
    "fastify": "^3.25.0",
    "fastify-autoload": "^3.9.0",
    "fastify-cors": "^6.0.2",
    "fastify-mongodb": "^4.1.1",
    "fastify-oas": "^3.0.8",
    "file-exists": "^5.0.1",
    "http": "^0.0.1-security",
    "mongodb": "^3.6.3",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "babel-jest": "^27.4.5",
    "jest": "^27.4.5"
  }
